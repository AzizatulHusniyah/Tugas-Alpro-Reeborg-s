#fungsi belok kanan
def turn_right():
    turn_left()
    turn_left()
    turn_left()

#fungsi untuk melewati tembok terendah
def bypass_wall_1():
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
    
#fungsi untuk melewati tembok tertinggi
def bypass_wall_2():
    turn_left()
    move()
    move()
    move()
    turn_right()
    move()
    turn_right()
    move()
    move()
    move()
    turn_left()
    
#fungsi untuk melewati tembok tertinggi kedua
def bypass_wall_3():
    turn_left()
    move()
    move()
    turn_right()
    move()
    turn_right()
    move()
    move()
    turn_left()
    
#fungsi untuk gerak robot secara keseluruhan
def gerak_keseluruhan():
    move()
    bypass_wall_2()
    bypass_wall_3()
    move()
    move()
    move()
    move()
    move()
    move()
    move()
    bypass_wall_1()
    bypass_wall_3()
    
#jalankan fungsi
gerak_keseluruhan()
