#Fungsi belok kanan
def turn_right():
    turn_left()
    turn_left()
    turn_left()
   
#fungsi lewati tembok
def lewati_tembok():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
  
#loop, menjalankan fungsi sampai finish
while not at_goal():
    lewati_tembok()
